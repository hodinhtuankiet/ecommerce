PORT=8000
# Local PostgreSQL
DB_HOST=localhost
DB_NAME=your_database_name
DB_USER=your_database_user
DB_PASSWORD=your_database_password
DB_PORT=5432
# CONFIG DOCKER
DB_HOST=db
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=123456
DB_NAME=postgres

# JWT Secrets (replace with your own secrets)
JWT_ACCESS_SECRET=your_access_secret
JWT_REFRESH_SECRET=your_refresh_secret
ACCESS_TOKEN_EXPIRY=15m
REFRESH_TOKEN_EXPIRY=7d

# Enable debug console logs (true/false)
DEBUG_CONSOLE=true

# Allow frontend URLs for CORS
FRONTEND_URLS=http://localhost:3000,http://127.0.0.1:3000,http://localhost:8000

# Optional: Supabase PostgreSQL (commented out if unused)
# DB_TYPE=postgres
# DB_HOST=your_supabase_host
# DB_PORT=6543
# DB_USER=your_supabase_user
# DB_PASSWORD=your_supabase_password
# DB_NAME=your_supabase_db
